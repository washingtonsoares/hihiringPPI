<aside id="grp_info_1" class="profile-nav col-lg-3">
  <%# render :partial => 'admin/users/profile' %>
</aside>

<aside id="admin_publications_posts" class="col-lg-9">

  <section id="list_all_publications" class="panel">

    <header id="titulo" class="panel-heading">
      Minhas Publicações
    </header>

    <div class="panel-body">
      <article class="media">
        <div id="publication_list_item" class="media-body blog-item">
          <ul id="list_publications" class="col-lg-12 col-sm-12">
            <% current_user.admin_publications.each do |admin_publication| %>
              <li class="col-lg-6">
                <div id="publication_list_thumb" class="">
                  <!--widget start-->
                  <section class="panel">

                    <div id="publication_list_thumb_layout" class="twt-feed">
                      <h1 id="titulo_publication_list"><%= retornaNome(admin_publication.user_id) %></h1>
                      <p id="email_publication_owner_list" class="">jsmith@flatlab.com</p>
                      <p id="owner_publication"><%= link_to (image_tag "http://graph.facebook.com/#{retornaThumb(admin_publication.user_id)}/picture?type=large", :class => 'owner_publication'), admin_user_path(admin_publication.user_id), class: 'thumb p-thumb' %></p>
                    </div>

                    <div id="publication_list_descript" class="weather-category twt-category">
                      <p><i class="icon icon-user"></i></p>
                      <h4 class="titulo_publication_list">
                        <%= link_to admin_user_publication_path(admin_publication.user_id, admin_publication.id) do %>
                          <%= truncate(admin_publication.title, length: 45) %>
                        <% end %>
                      </h4>
                      <p class="descrip_public">
                        <%= truncate(admin_publication.resume, length: 90) { link_to ' visualizar', admin_user_publication_path(retornaNomeFormatado(admin_publication.user_id), admin_publication) } %>
                      </p>
                    </div>

                  </section>
                  <!--widget end-->
                </div>
              </li>
            <% end %>
          </ul>
        </div>
      </article>
    </div>

  </section>


  <footer>
    <%# will_paginate @admin_publications %>
  </footer>
</aside>