<h1>Listing admin_publications</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Resume</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_publications.each do |admin_publication| %>
      <tr>
        <td><%= admin_publication.title %></td>
        <td><%= admin_publication.resume %></td>
        <td><%= admin_publication.user %></td>
        <td><%= link_to 'Show', admin_publication %></td>
        <td><%= link_to 'Edit', edit_admin_publication_path(admin_publication) %></td>
        <td><%= link_to 'Destroy', admin_publication, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publication', new_admin_publication_path %>
